{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            em.id,\n            em.source_entity_type,\n            em.source_entity_id,\n            em.entity_type,\n            em.entity_id,\n            em.user_id,\n            em.created_at\n        FROM comms_entity_mentions AS em\n        JOIN comms_messages AS m ON em.source_entity_id = m.id::text\n        WHERE em.source_entity_type = 'message'\n          AND (m.id = $1 OR m.thread_id = $1)\n        ORDER BY em.created_at ASC\n        ",
