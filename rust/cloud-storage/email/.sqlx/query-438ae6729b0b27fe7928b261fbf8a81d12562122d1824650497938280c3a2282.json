{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            a.id,\n            a.message_id,\n            a.item_id as \"item_id!\",\n            a.item_type as \"item_type!\",\n            a.created_at,\n            m.thread_id\n        FROM\n            email_attachments_macro a\n        JOIN\n            email_messages m ON a.message_id = m.id\n        WHERE\n            m.thread_id = ANY($1)\n        ORDER BY\n            a.created_at ASC\n        ",
