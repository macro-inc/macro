{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            m.id,\n            m.provider_id,\n            m.global_id,\n            m.link_id,\n            m.thread_id,\n            m.provider_thread_id,\n            m.replying_to_id,\n            m.provider_history_id,\n            m.internal_date_ts,\n            m.snippet,\n            m.size_estimate,\n            m.subject,\n            m.from_contact_id,\n            m.sent_at,\n            m.has_attachments,\n            m.is_read,\n            m.is_starred,\n            m.is_sent,\n            m.is_draft,\n            NULL::TEXT as body_text,\n            NULL::TEXT as body_html_sanitized,\n            NULL::TEXT as body_macro,\n            m.headers_jsonb,\n            m.created_at,\n            m.updated_at\n        FROM\n            email_messages m\n        WHERE\n            m.thread_id = $1 AND m.link_id = $2\n        ORDER BY\n            m.internal_date_ts DESC NULLS LAST\n        ",
