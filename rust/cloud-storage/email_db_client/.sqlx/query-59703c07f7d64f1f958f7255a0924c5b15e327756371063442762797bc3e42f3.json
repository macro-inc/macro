  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            l.id, \n            l.link_id, \n            l.provider_label_id, \n            l.name, \n            l.created_at,\n            l.message_list_visibility as \"message_list_visibility: _\",\n            l.label_list_visibility as \"label_list_visibility: _\",\n            l.type as \"type_: _\"\n        FROM email_message_labels ml\n        JOIN email_labels l ON ml.label_id = l.id\n        WHERE ml.message_id = $1\n        ORDER BY l.name\n        ",
