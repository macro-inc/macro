  "query": "\n            SELECT\n                p.id as id,\n                p.name,\n                p.\"userId\" as user_id,\n                p.\"parentId\" as \"parent_id?\",\n                p.\"createdAt\"::timestamptz as created_at,\n                p.\"updatedAt\"::timestamptz as updated_at,\n                p.\"deletedAt\"::timestamptz as deleted_at\n            FROM\n                \"Project\" p\n            WHERE p.\"parentId\" = $1 AND p.\"deletedAt\" IS NULL\n        ",
