  "query": "\n        SELECT \n            t.id as thread_id, \n            t.resolved, \n            t.\"documentId\" as document_id, \n            t.\"createdAt\"::timestamptz as created_at, \n            t.\"updatedAt\"::timestamptz as updated_at, \n            t.\"deletedAt\"::timestamptz as deleted_at, \n            t.metadata, \n            t.owner\n        FROM \"Thread\" t\n        WHERE t.\"id\" = $1\n            AND t.\"deletedAt\" IS NULL\n        ",
