{
  "name": "@coparse/monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "infra/*"
  ],
  "packageManager": "bun@1.3.0",
  "scripts": {
    "dev": "cd packages/app && bun run --bun dev",
    "local-dcs": "cd packages/app && VITE_LOCAL_SERVERS='cognition-service,cognition-websocket-service' bun run --bun dev",
    "local-dss": "cd packages/app && VITE_LOCAL_SERVERS='document-storage-service' bun run --bun dev",
    "local-search": "cd packages/app && VITE_LOCAL_SERVERS='search-service' bun run --bun dev",
    "check": "tsc --noEmit --skipLibCheck --project packages/app/tsconfig.json",
    "build": "cd packages/app && MODE=development NODE_ENV=production bun run --bun build",
    "build:dev": "bun run build",
    "build:staging": "cd packages/app && MODE=staging NODE_ENV=production bun run --bun build",
    "build:prod": "cd packages/app && MODE=production NODE_ENV=production bun run --bun build",
    "preview": "cd packages/app && bun run --bun preview:dev",
    "preview:prod": "cd packages/app && MODE=production NODE_ENV=production bun preview:prod",
    "lint": "bunx --bun biome lint --skip=nursery/noImportCycles",
    "lint:staged": "bunx --bun biome lint --staged",
    "lint:ci": "bunx --bun biome ci --changed",
    "format": "bunx --bun biome check --linter-enabled=false --write --staged",
    "check-tailwind": "bun scripts/check-tailwind.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "java:build": "bun scripts/createBackend.js",
    "csharp:build": "bun scripts/publish-docx-server.js",
    "clean-env": "bun scripts/clean-env.js",
    "cycles": "bunx --bun biome lint --only=nursery/noImportCycles",
    "cycles:ci": "bunx --bun biome lint --changed --only=nursery/noImportCycles",
    "pkg": "bun scripts/pkg.ts",
    "gen-api": "bun scripts/generate-api-schema.ts",
    "gen-dcs-types": "bun scripts/generate-dcs-types.ts",
    "gen-dcs-tools": "bun scripts/generate-dcs-tools.ts",
    "gen-dcs-models": "bun scripts/generate-dcs-models.ts",
    "fix-lock": "git fetch origin dev:dev && (git checkout dev -- bun.lock && bun i && git add bun.lock)",
    "publish-lexical": "cd packages/lexical-core && bun publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
